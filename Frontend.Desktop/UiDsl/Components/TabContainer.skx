<view id="{id}" x="{x}" y="{y}" width="{width}" height="{height}">
  <view x="0" y="0" width="{width}" height="40" backgroundColor="#F1F5F9">
    <slot name="tabs" />
  </view>
  
  <view x="0" y="40" width="{width}" height="{height - 40}" backgroundColor="#FFFFFF" borderColor="#CBD5E1" borderWidth="1">
    <slot />
  </view>
</view> 